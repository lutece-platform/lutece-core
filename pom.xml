        <!-- compilation can be broken by digester's new usage of generics -->
        <dependency>
            <groupId>commons-digester</groupId>
            <artifactId>commons-digester</artifactId>
            <version>1.8.1</version>
        </dependency>

        <dependency>       
            <groupId>org.apache.commons</groupId>
